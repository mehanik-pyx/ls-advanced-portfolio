.socials {
  display: flex;
  align-items: center;
}

.socials__item {
  margin-right: 40px;

  &:last-child {
    margin-right: 0;
  }
}

.socials__icon {
  width: 29px;
  height: 29px;
  display: block;
  fill: $text-color;
}

.scroll__icon {
  width: 30px;
  height: 30px;
  display: block;
  fill: $text-color;
}